<div class="main-container">
  <div class="image-row">
    <img class="avatar-image" src="profile_pic.PNG" target="Chad Shimozaki headshot"></img>
  </div> <!-- /image-row -->

  <div class="name-row">
    Chad Shimozaki
  </div> <!-- /name-row -->

  <div class="social-icons-row">
    <a href="https://www.linkedin.com/in/chadshimozaki/" target="_blank">
      <i class="fa-brands fa-linkedin"></i> LinkedIn
    </a>

    <a href="https://www.instagram.com/chad_shimo14/profilecard/?igsh=NDY0MjNwamFzcXBq" target="_blank">
      <i class="fa-brands fa-square-instagram"></i> Instagram
    </a>

    <a href="https://github.com/chad-shimozaki" target="_blank">
      <i class="fa-brands fa-github"></i> GitHub
    </a>
  </div> <!-- /social-icons-row -->

  <div class="links-row">
    <% @list_of_items.each do |the_item| %>
      <div class="item">
        <img src="<%= the_item.thumbnail_url %>" class="thumbnail">

        <a class="link-text" target="_blank" href ="<%= the_item.link_url %>">
          <%= the_item.link_description %>
        </a>
      </div>
    <% end %>
  </div> <!-- /links-row -->
</div> <!-- /main-container -->
